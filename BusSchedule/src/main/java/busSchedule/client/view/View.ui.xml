<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:VerticalPanel ui:field="mainPanel">
        <g:FlowPanel ui:field="tablePanel"/>
        <g:HorizontalPanel>
            <g:Button ui:field="previousPage" text="Previous"/>
            <g:Button ui:field="nextPage" text="Next"/>
            <g:Label ui:field="label"/>
        </g:HorizontalPanel>
        <g:HorizontalPanel>
            <g:Button ui:field="addRow" text="Add"/>
            <g:Button ui:field="deleteRow" text="Delete"/>
        </g:HorizontalPanel>
    </g:VerticalPanel>

</ui:UiBinder>